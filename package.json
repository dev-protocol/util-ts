{
	"name": "@devprtcl/util-ts",
	"version": "1.4.0",
	"description": "TypeScript Utilities",
	"main": "cjs/index.js",
	"module": "esm/index.js",
	"scripts": {
		"test": "ava",
		"build": "yarn clean && run-p -l build:* && tsc",
		"build:cjs": "tsc --project . --module commonjs --outDir ./cjs --declaration",
		"build:esm": "tsc --project . --module es2015 --outDir ./esm --declaration",
		"watch": "tsc -w",
		"pretest": "yarn build",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts --fix",
		"lint:format": "prettier --write '**/*.{ts,js,json,md,yml}'",
		"prepack": "yarn test",
		"clean": "rimraf esm cjs dist"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"bent": "7.3.12",
		"ethereum-waffle": "3.2.1",
		"ramda": "^0.27.1"
	},
	"files": [
		"cjs",
		"esm"
	],
	"devDependencies": {
		"@ava/typescript": "1.1.1",
		"@types/bent": "7.3.2",
		"@types/dotenv": "8.2.0",
		"@types/node": "14.14.13",
		"@types/ramda": "0.27.33",
		"@typescript-eslint/eslint-plugin": "4.9.1",
		"@typescript-eslint/parser": "4.9.1",
		"ava": "3.14.0",
		"dotenv": "8.2.0",
		"eslint": "7.15.0",
		"eslint-config-prettier": "6.15.0",
		"eslint-plugin-functional": "3.1.0",
		"husky": "4.3.5",
		"npm-run-all": "4.1.5",
		"prettier": "2.2.1",
		"rimraf": "3.0.2",
		"typescript": "4.1.3"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			}
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/utils.git"
	},
	"bugs": {
		"url": "https://github.com/dev-protocol/utils/issues"
	},
	"homepage": "https://github.com/dev-protocol/utils#readme"
}
